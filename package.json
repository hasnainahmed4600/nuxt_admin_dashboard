{
  "name": "Admin-Portal",
  "version": "1.1.1",
  "private": true,
  "description": "Yalla Dealz Admin Portal",
  "author": "Yalla Dealz Inc",
  "scripts": {
    "build": "nuxt build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "node_modules/.bin/nuxt",
    "generate": "nuxt generate",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "sass": "sass --watch assets/scss/custom.scss:assets/css/bootstrap.css assets/scss/main-style.scss:assets/css/main-style.css",
    "start": "nuxt start",
    "test": "jest",
    "release": "release-it"
  },
  "dependencies": {
    "@babel/preset-env": "^7.11.5",
    "@johmun/vue-tags-input": "^2.1.0",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@vue/test-utils": "^1.0.5",
    "babel-jest": "^26.3.0",
    "body-parser": "^1.19.0",
    "bootstrap-vue": "^2.15.0",
    "chartist": "^0.11.4",
    "fibers": "^5.0.0",
    "form-data": "^3.0.0",
    "js-cookie": "^2.2.1",
    "node-sass": "^4.14.1",
    "nuxt": "^2.14.4",
    "nuxt-ctk-date-time-picker": "^2.1.1",
    "nuxt-property-decorator": "^2.7.2",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.12",
    "vue-analytics": "^5.22.1",
    "vue-class-component": "^7.2.4",
    "vue-cookie": "^1.1.4",
    "vue-css-donut-chart": "^1.3.0",
    "vue-jest": "^3.0.5",
    "vue-property-decorator": "^9.0.0",
    "vue-qrcode": "^0.3.3",
    "vue-server-renderer": "^2.6.12",
    "vue2-editor": "^2.10.2",
    "vue2-google-maps": "^0.10.7",
    "vuelidate": "^0.7.5",
    "vuetify-datetime-picker": "^2.1.1",
    "vuetify-google-autocomplete": "^2.0.1",
    "vuex-module-decorators": "^0.17.0"
  },
  "release-it": {
    "git": {
      "changelog": "git log --pretty=format:\"* %s (%h)\" ${from}...${to}",
      "requireCleanWorkingDir": false,
      "requireBranch": false,
      "requireUpstream": true,
      "requireCommits": false,
      "addUntrackedFiles": false,
      "commit": true,
      "commitMessage": "Release ${version}",
      "commitArgs": [],
      "tag": true,
      "tagName": null,
      "tagAnnotation": "Release ${version}",
      "tagArgs": [],
      "push": true,
      "pushArgs": [
        "--follow-tags"
      ],
      "pushRepo": ""
    }
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.4",
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/vuelidate": "^0.7.13",
    "@types/webpack-env": "^1.15.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.0",
    "jest": "^26.4.2",
    "release-it": "^13.7.0",
    "typescript": "^3.9.7",
    "vue-browser-geolocation": "^1.8.0",
    "vue-template-compiler": "^2.6.12"
  }
}
